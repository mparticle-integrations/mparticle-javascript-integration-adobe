{
  "name": "mparticle-adobe-heartbeat",
  "version": "1.0.0",
  "description": "mParticle Adobe Heartbeat Kit Integration",
  "repository": {
    "type": "git",
    "url": "https://github.com/mparticle-integrations/mparticle-javascript-integration-adobe-heartbeat.git"
  },
  "scripts": {
    "testKarma": "node test/boilerplate/test-karma.js",
    "build": "rollup --config rollup.config.js",
